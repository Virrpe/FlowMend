{
  "name": "flowmend",
  "version": "1.0.0",
  "description": "Shopify app for safe bulk metafield operations at scale via Flow actions",
  "type": "module",
  "scripts": {
    "dev": "remix dev",
    "build": "prisma generate && remix build",
    "start": "remix-serve build",
    "deploy": "prisma migrate deploy && npm run start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx app server",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "worker:dev": "tsx watch server/jobs/worker.ts",
    "worker:start": "node dist/worker.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "@shopify/polaris": "^12.16.0",
    "@shopify/shopify-api": "^9.0.1",
    "@shopify/shopify-app-remix": "^2.6.0",
    "bullmq": "^5.1.2",
    "graphql-request": "^6.1.0",
    "ioredis": "^5.3.2",
    "isbot": "^4",
    "pino": "^8.16.2",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "undici": "^7.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@shopify/app": "^3.58.2",
    "@shopify/cli": "^3.88.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
